<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>PI-Infnet-5-Ciclo</display-name>

	<servlet>
		<servlet-name>IndexServlet</servlet-name>
		<servlet-class>servlets.IndexServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IndexServlet</servlet-name>
		<url-pattern>/inicio.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DetalheServlet</servlet-name>
		<servlet-class>servlets.DetalheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DetalheServlet</servlet-name>
		<url-pattern>/detalhe.do</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>inicio.do</welcome-file>
	</welcome-file-list>
</web-app>